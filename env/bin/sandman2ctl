#!/Users/ak/Documents/_CodingNomads/python-onsite/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'sandman2==1.2.0','console_scripts','sandman2ctl'
__requires__ = 'sandman2==1.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sandman2==1.2.0', 'console_scripts', 'sandman2ctl')()
    )
